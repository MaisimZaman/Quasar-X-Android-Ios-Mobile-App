{
  "expo": {
    "name": "QuasarX-Mobile",
    "slug": "QuasarX-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./services/assets/mainlogo.png",
    "splash": {
      "image": "./services/assets/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "QuasarX.apk"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./services/assets/mainlogo.png",
        "backgroundColor": "#000000"
      },
      "package": "quasarX.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "this is an App where you can say facts without getting banned."
  }
}
